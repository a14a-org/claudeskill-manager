{
  "name": "@claudeskill/core",
  "version": "0.1.0",
  "description": "Core encryption and skill management for Claude Skill Sync",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "node --test --experimental-strip-types src/**/*.test.ts",
    "clean": "rm -rf dist .turbo"
  },
  "dependencies": {
    "@noble/hashes": "^2.0.1"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a14a-org/claudeskill-manager"
  },
  "license": "MIT"
}
